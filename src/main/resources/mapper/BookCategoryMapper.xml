        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xs.booksystem.mapper.BookCategoryMapper">

    <resultMap id="BaseResultMap" type="com.xs.booksystem.pojo.DTO.BookCategoryDTO">
            <id property="id" column="id" jdbcType="INTEGER"/>
